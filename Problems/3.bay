def isPrime(int num)
{
  bool check = True.
  int i
  for(i=1;i<=num/2;i+=1)
  {
    if(num%i == 0)
    {
      check = False
    }
  }
  return check.
}
def main(void)
{
  int sum = 0.
  int i.
  int n.
  scan(n)
  for(i=0; i < n; i+=1)
  {
    if(isPrime(i))
    {
      sum = sum + i
    }
  }
  return void.
}
