def pythogoreanTriplets(int maxLim)
{
	int a = 0.
	int b = 0.
	int c = 0.
	int m = 2.
	int n.
	int count = 0
	while(c < maxLim)
	{
		for(n=1;n<m;n=n+1)
		{
			a = m*m - n*n.
			b = 2*m*n.
			c = m*m + n*n
			if(c>maxLim)
			{
				break.
			}
			count=count+1
		}
		m=m+1
	}
	return count.
}

def main(void)
{
	int n.
	scan(n).
	print(pythogoreanTriplets(n))
	return void .
}
